<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_graph"
    app:startDestination="@id/stockFragment">

    <fragment
        android:id="@+id/stockFragment"
        android:name="xyz.zedler.patrick.grocy.fragment.StockFragment"
        android:label="StockFragment">
        <action
            android:id="@+id/action_stockFragment_to_productOverviewBottomSheetDialogFragment"
            app:destination="@id/productOverviewBottomSheetDialogFragment" />
        <action
            android:id="@+id/action_stockFragment_to_scanInputFragment"
            app:destination="@id/scanInputFragment"
            app:enterAnim="@anim/slide_in_up"
            app:exitAnim="@anim/slide_no"
            app:popExitAnim="@anim/slide_out_down" />
    </fragment>
    <fragment
        android:id="@+id/shoppingListFragment"
        android:name="xyz.zedler.patrick.grocy.fragment.ShoppingListFragment"
        android:label="ShoppingListFragment">
        <action
            android:id="@+id/action_shoppingListFragment_to_shoppingListItemEditFragment"
            app:destination="@id/shoppingListItemEditFragment"
            app:enterAnim="@anim/slide_in_up"
            app:exitAnim="@anim/slide_no"
            app:popExitAnim="@anim/slide_out_down" />
        <action
            android:id="@+id/action_shoppingListFragment_to_purchaseFragment"
            app:destination="@id/purchaseFragment"
            app:enterAnim="@anim/slide_in_up"
            app:exitAnim="@anim/slide_no"
            app:popExitAnim="@anim/slide_out_down" />
        <action
            android:id="@+id/action_shoppingListFragment_to_shoppingListEditFragment"
            app:destination="@id/shoppingListEditFragment"
            app:enterAnim="@anim/slide_in_up"
            app:exitAnim="@anim/slide_no"
            app:popExitAnim="@anim/slide_out_down" />
    </fragment>
    <fragment
        android:id="@+id/consumeFragment"
        android:name="xyz.zedler.patrick.grocy.fragment.ConsumeFragment"
        android:label="ConsumeFragment">
        <argument
            android:name="closeWhenFinished"
            android:defaultValue="false"
            app:argType="boolean" />
        <argument
            android:name="productName"
            android:defaultValue="@null"
            app:argType="string"
            app:nullable="true" />
        <action
            android:id="@+id/action_consumeFragment_to_inputNameBottomSheetDialogFragment"
            app:destination="@id/inputNameBottomSheetDialogFragment" />
        <action
            android:id="@+id/action_consumeFragment_to_productOverviewBottomSheetDialogFragment"
            app:destination="@id/productOverviewBottomSheetDialogFragment" />
        <action
            android:id="@+id/action_consumeFragment_to_scanBatchFragment"
            app:destination="@id/scanBatchFragment"
            app:enterAnim="@anim/slide_in_up"
            app:exitAnim="@anim/slide_no"
            app:popExitAnim="@anim/slide_out_down" />
        <action
            android:id="@+id/action_consumeFragment_to_scanInputFragment"
            app:destination="@id/scanInputFragment"
            app:enterAnim="@anim/slide_in_up"
            app:exitAnim="@anim/slide_no"
            app:popExitAnim="@anim/slide_out_down" />
    </fragment>
    <fragment
        android:id="@+id/purchaseFragment"
        android:name="xyz.zedler.patrick.grocy.fragment.PurchaseFragment"
        android:label="PurchaseFragment">
        <argument
            android:name="closeWhenFinished"
            android:defaultValue="false"
            app:argType="boolean" />
        <argument
            android:name="productName"
            android:defaultValue="@null"
            app:argType="string"
            app:nullable="true" />
        <action
            android:id="@+id/action_purchaseFragment_to_inputNameBottomSheetDialogFragment"
            app:destination="@id/inputNameBottomSheetDialogFragment" />
        <argument
            android:name="amount"
            android:defaultValue="@null"
            app:argType="string"
            app:nullable="true" />
        <action
            android:id="@+id/action_purchaseFragment_to_productOverviewBottomSheetDialogFragment"
            app:destination="@id/productOverviewBottomSheetDialogFragment" />
        <action
            android:id="@+id/action_purchaseFragment_to_scanBatchFragment"
            app:destination="@id/scanBatchFragment"
            app:enterAnim="@anim/slide_in_up"
            app:exitAnim="@anim/slide_no"
            app:popExitAnim="@anim/slide_out_down" />
        <action
            android:id="@+id/action_purchaseFragment_to_scanInputFragment"
            app:destination="@id/scanInputFragment"
            app:enterAnim="@anim/slide_in_up"
            app:exitAnim="@anim/slide_no"
            app:popExitAnim="@anim/slide_out_down" />
        <action
            android:id="@+id/action_purchaseFragment_to_masterProductSimpleFragment"
            app:destination="@id/masterProductSimpleFragment"
            app:enterAnim="@anim/slide_in_up"
            app:exitAnim="@anim/slide_no"
            app:popExitAnim="@anim/slide_out_down" />
        <argument
            android:name="shoppingListItems"
            android:defaultValue="@null"
            app:argType="xyz.zedler.patrick.grocy.model.ShoppingListItem[]"
            app:nullable="true" />
        <argument
            android:name="animateStart"
            android:defaultValue="true"
            app:argType="boolean" />
    </fragment>
    <activity
        android:id="@+id/settingsActivity"
        android:name="xyz.zedler.patrick.grocy.activity.SettingsActivity"
        android:label="SettingsActivity" />
    <fragment
        android:id="@+id/masterProductsFragment"
        android:name="xyz.zedler.patrick.grocy.fragment.MasterProductsFragment"
        android:label="MasterProductsFragment">
        <action
            android:id="@+id/action_masterProductsFragment_to_masterProductSimpleFragment"
            app:destination="@id/masterProductSimpleFragment"
            app:enterAnim="@anim/slide_in_up"
            app:exitAnim="@anim/slide_no"
            app:popExitAnim="@anim/slide_out_down" />
    </fragment>
    <fragment
        android:id="@+id/masterStoresFragment"
        android:name="xyz.zedler.patrick.grocy.fragment.MasterStoresFragment"
        android:label="MasterStoresFragment">
        <action
            android:id="@+id/action_masterStoresFragment_to_masterStoreFragment"
            app:destination="@id/masterStoreFragment"
            app:enterAnim="@anim/slide_in_up"
            app:exitAnim="@anim/slide_no"
            app:popExitAnim="@anim/slide_out_down" />
    </fragment>
    <fragment
        android:id="@+id/masterLocationsFragment"
        android:name="xyz.zedler.patrick.grocy.fragment.MasterLocationsFragment"
        android:label="MasterLocationsFragment">
        <action
            android:id="@+id/action_masterLocationsFragment_to_masterLocationFragment"
            app:destination="@id/masterLocationFragment"
            app:enterAnim="@anim/slide_in_up"
            app:exitAnim="@anim/slide_no"
            app:popExitAnim="@anim/slide_out_down" />
    </fragment>
    <fragment
        android:id="@+id/masterQuantityUnitsFragment"
        android:name="xyz.zedler.patrick.grocy.fragment.MasterQuantityUnitsFragment"
        android:label="MasterQuantityUnitsFragment">
        <action
            android:id="@+id/action_masterQuantityUnitsFragment_to_masterQuantityUnitFragment"
            app:destination="@id/masterQuantityUnitFragment"
            app:enterAnim="@anim/slide_in_up"
            app:exitAnim="@anim/slide_no"
            app:popExitAnim="@anim/slide_out_down" />
    </fragment>
    <fragment
        android:id="@+id/masterProductGroupsFragment"
        android:name="xyz.zedler.patrick.grocy.fragment.MasterProductGroupsFragment"
        android:label="MasterProductGroupsFragment">
        <action
            android:id="@+id/action_masterProductGroupsFragment_to_masterProductGroupFragment"
            app:destination="@id/masterProductGroupFragment"
            app:enterAnim="@anim/slide_in_up"
            app:exitAnim="@anim/slide_no"
            app:popExitAnim="@anim/slide_out_down" />
    </fragment>
    <dialog
        android:id="@+id/feedbackBottomSheetDialogFragment"
        android:name="xyz.zedler.patrick.grocy.fragment.bottomSheetDialog.FeedbackBottomSheet"
        android:label="FeedbackBottomSheetDialogFragment" />
    <fragment
        android:id="@+id/masterProductSimpleFragment"
        android:name="xyz.zedler.patrick.grocy.fragment.MasterProductSimpleFragment"
        android:label="MasterProductSimpleFragment">
        <argument
            android:name="action"
            app:argType="string" />
        <argument
            android:name="product"
            android:defaultValue="@null"
            app:argType="xyz.zedler.patrick.grocy.model.Product"
            app:nullable="true" />
        <argument
            android:name="createProductObject"
            android:defaultValue="@null"
            app:argType="xyz.zedler.patrick.grocy.model.CreateProduct"
            app:nullable="true" />
        <action
            android:id="@+id/action_masterProductSimpleFragment_to_scanInputFragment"
            app:destination="@id/scanInputFragment"
            app:enterAnim="@anim/slide_in_up"
            app:exitAnim="@anim/slide_no"
            app:popExitAnim="@anim/slide_out_down" />
    </fragment>
    <fragment
        android:id="@+id/masterLocationFragment"
        android:name="xyz.zedler.patrick.grocy.fragment.MasterLocationFragment"
        android:label="MasterLocationFragment" />
    <fragment
        android:id="@+id/masterStoreFragment"
        android:name="xyz.zedler.patrick.grocy.fragment.MasterStoreFragment"
        android:label="MasterStoreFragment" />
    <fragment
        android:id="@+id/masterQuantityUnitFragment"
        android:name="xyz.zedler.patrick.grocy.fragment.MasterQuantityUnitFragment"
        android:label="MasterQuantityUnitFragment" />
    <fragment
        android:id="@+id/masterProductGroupFragment"
        android:name="xyz.zedler.patrick.grocy.fragment.MasterProductGroupFragment"
        android:label="MasterProductGroupFragment" />
    <fragment
        android:id="@+id/shoppingListItemEditFragment"
        android:name="xyz.zedler.patrick.grocy.fragment.ShoppingListItemEditFragment"
        android:label="ShoppingListItemEditFragment">
        <argument
            android:name="selectedShoppingListId"
            android:defaultValue="-1"
            app:argType="integer" />
        <argument
            android:name="action"
            app:argType="string" />
        <action
            android:id="@+id/action_shoppingListItemEditFragment_to_productOverviewBottomSheetDialogFragment"
            app:destination="@id/productOverviewBottomSheetDialogFragment" />
        <argument
            android:name="productName"
            android:defaultValue="@null"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="shoppingListItem"
            android:defaultValue="@null"
            app:argType="xyz.zedler.patrick.grocy.model.ShoppingListItem"
            app:nullable="true" />
        <action
            android:id="@+id/action_shoppingListItemEditFragment_to_scanInputFragment"
            app:destination="@id/scanInputFragment"
            app:enterAnim="@anim/slide_in_up"
            app:exitAnim="@anim/slide_no"
            app:popExitAnim="@anim/slide_out_down" />
    </fragment>
    <dialog
        android:id="@+id/productOverviewBottomSheetDialogFragment"
        android:name="xyz.zedler.patrick.grocy.fragment.bottomSheetDialog.ProductOverviewBottomSheet"
        android:label="ProductOverviewBottomSheetDialogFragment">
        <action
            android:id="@+id/action_productOverviewBottomSheetDialogFragment_to_masterProductSimpleFragment"
            app:destination="@id/masterProductSimpleFragment"
            app:enterAnim="@anim/slide_in_up"
            app:exitAnim="@anim/slide_no"
            app:popExitAnim="@anim/slide_out_down" />
        <action
            android:id="@+id/action_productOverviewBottomSheetDialogFragment_to_shoppingListItemEditFragment"
            app:destination="@id/shoppingListItemEditFragment"
            app:enterAnim="@anim/slide_in_up"
            app:exitAnim="@anim/slide_no"
            app:popExitAnim="@anim/slide_out_down" />
        <argument
            android:name="productDetails"
            android:defaultValue="@null"
            app:argType="xyz.zedler.patrick.grocy.model.ProductDetails"
            app:nullable="true" />
        <argument
            android:name="stockItem"
            android:defaultValue="@null"
            app:argType="xyz.zedler.patrick.grocy.model.StockItem"
            app:nullable="true" />
        <argument
            android:name="quantityUnit"
            android:defaultValue="@null"
            app:argType="xyz.zedler.patrick.grocy.model.QuantityUnit"
            app:nullable="true" />
        <argument
            android:name="location"
            android:defaultValue="@null"
            app:argType="xyz.zedler.patrick.grocy.model.Location"
            app:nullable="true" />
        <argument
            android:name="showActions"
            android:defaultValue="false"
            app:argType="boolean" />
        <action
            android:id="@+id/action_productOverviewBottomSheetDialogFragment_to_consumeFragment"
            app:destination="@id/consumeFragment"
            app:enterAnim="@anim/slide_in_up"
            app:exitAnim="@anim/slide_no"
            app:popExitAnim="@anim/slide_out_down" />
        <action
            android:id="@+id/action_productOverviewBottomSheetDialogFragment_to_purchaseFragment"
            app:destination="@id/purchaseFragment"
            app:enterAnim="@anim/slide_in_up"
            app:exitAnim="@anim/slide_no"
            app:popExitAnim="@anim/slide_out_down" />
    </dialog>
    <dialog
        android:id="@+id/drawerBottomSheetDialogFragment"
        android:name="xyz.zedler.patrick.grocy.fragment.bottomSheetDialog.DrawerBottomSheet"
        android:label="DrawerBottomSheetDialogFragment">
        <action
            android:id="@+id/action_drawerBottomSheetDialogFragment_to_shoppingListFragment"
            app:destination="@id/shoppingListFragment" />
        <action
            android:id="@+id/action_drawerBottomSheetDialogFragment_to_purchaseFragment"
            app:destination="@id/purchaseFragment" />
        <action
            android:id="@+id/action_drawerBottomSheetDialogFragment_to_consumeFragment"
            app:destination="@id/consumeFragment" />
        <action
            android:id="@+id/action_drawerBottomSheetDialogFragment_to_settingsActivity"
            app:destination="@id/settingsActivity" />
        <action
            android:id="@+id/action_drawerBottomSheetDialogFragment_to_feedbackBottomSheetDialogFragment"
            app:destination="@id/feedbackBottomSheetDialogFragment" />
        <action
            android:id="@+id/action_drawerBottomSheetDialogFragment_to_scanBatchFragment"
            app:destination="@id/scanBatchFragment" />
        <action
            android:id="@+id/action_drawerBottomSheetDialogFragment_to_settingsFragment"
            app:destination="@id/settingsOverviewFragment" />
    </dialog>
    <action
        android:id="@+id/action_global_drawerBottomSheetDialogFragment"
        app:destination="@id/drawerBottomSheetDialogFragment" />
    <dialog
        android:id="@+id/inputNameBottomSheetDialogFragment"
        android:name="xyz.zedler.patrick.grocy.fragment.bottomSheetDialog.InputNameBottomSheet"
        android:label="InputNameBottomSheetDialogFragment">
        <argument
            android:name="productName"
            app:argType="string" />
        <action
            android:id="@+id/action_inputNameBottomSheetDialogFragment_to_masterProductSimpleFragment"
            app:destination="@id/masterProductSimpleFragment"
            app:enterAnim="@anim/slide_in_up"
            app:exitAnim="@anim/slide_no"
            app:popExitAnim="@anim/slide_out_down" />
    </dialog>
    <fragment
        android:id="@+id/missingBatchItemsFragment"
        android:name="xyz.zedler.patrick.grocy.fragment.MissingBatchItemsFragment"
        android:label="MissingBatchItemsFragment">
        <argument
            android:name="missingBatchItems"
            app:argType="xyz.zedler.patrick.grocy.model.MissingBatchItem[]" />
    </fragment>
    <fragment
        android:id="@+id/scanBatchFragment"
        android:name="xyz.zedler.patrick.grocy.fragment.ScanBatchFragment"
        android:label="ScanBatchFragment">
        <argument
            android:name="action"
            app:argType="string" />
        <argument
            android:name="startAnimated"
            android:defaultValue="true"
            app:argType="boolean" />
    </fragment>
    <fragment
        android:id="@+id/scanInputFragment"
        android:name="xyz.zedler.patrick.grocy.fragment.ScanInputFragment"
        android:label="ScanInputFragment" />
    <fragment
        android:id="@+id/logFragment"
        android:name="xyz.zedler.patrick.grocy.fragment.LogFragment"
        android:label="LogFragment" />
    <fragment
        android:id="@+id/aboutFragment"
        android:name="xyz.zedler.patrick.grocy.fragment.AboutFragment"
        android:label="AboutFragment">
        <action
            android:id="@+id/action_aboutFragment_to_featuresFragment"
            app:destination="@id/featuresFragment"
            app:enterAnim="@anim/slide_in_up"
            app:exitAnim="@anim/slide_no"
            app:popExitAnim="@anim/slide_out_down" />
    </fragment>
    <fragment
        android:id="@+id/shoppingListEditFragment"
        android:name="xyz.zedler.patrick.grocy.fragment.ShoppingListEditFragment"
        android:label="ShoppingListEditFragment" />
    <fragment
        android:id="@+id/loginFragment"
        android:name="xyz.zedler.patrick.grocy.fragment.LoginFragment"
        android:label="LoginFragment">
        <action
            android:id="@+id/action_loginFragment_to_aboutFragment"
            app:destination="@id/aboutFragment"
            app:enterAnim="@anim/slide_in_up"
            app:exitAnim="@anim/slide_no"
            app:popExitAnim="@anim/slide_out_down" />
    </fragment>
    <action
        android:id="@+id/action_global_loginFragment"
        app:destination="@id/loginFragment"
        app:enterAnim="@anim/slide_no"
        app:exitAnim="@anim/slide_no"
        app:popEnterAnim="@anim/slide_from_right"
        app:popExitAnim="@anim/slide_to_left" />
    <fragment
        android:id="@+id/featuresFragment"
        android:name="xyz.zedler.patrick.grocy.fragment.FeaturesFragment"
        android:label="FeaturesFragment" />
    <action
        android:id="@+id/action_global_featuresFragment"
        app:destination="@id/featuresFragment"
        app:enterAnim="@anim/slide_no"
        app:exitAnim="@anim/slide_no"
        app:popEnterAnim="@anim/slide_from_right"
        app:popExitAnim="@anim/slide_to_left" />
    <action
        android:id="@+id/action_global_scanBatchFragment"
        app:destination="@id/scanBatchFragment"
        app:enterAnim="@anim/slide_no"
        app:exitAnim="@anim/slide_no"
        app:popExitAnim="@anim/slide_out_down"
        app:popUpTo="@id/stockFragment"
        app:popUpToInclusive="false" />
    <fragment
        android:id="@+id/settingsOverviewFragment"
        android:name="xyz.zedler.patrick.grocy.fragment.SettingsOverviewFragment"
        android:label="SettingsOverviewFragment">
        <action
            android:id="@+id/action_settingsOverviewFragment_to_settingsFragment"
            app:destination="@id/settingsFragment"
            app:enterAnim="@anim/slide_from_right"
            app:exitAnim="@anim/slide_to_left"
            app:popEnterAnim="@anim/slide_from_left"
            app:popExitAnim="@anim/slide_to_right" />
        <action
            android:id="@+id/action_settingsOverviewFragment_to_aboutFragment"
            app:destination="@id/aboutFragment"
            app:enterAnim="@anim/slide_from_right"
            app:exitAnim="@anim/slide_to_left"
            app:popEnterAnim="@anim/slide_from_left"
            app:popExitAnim="@anim/slide_to_right" />
    </fragment>
    <fragment
        android:id="@+id/settingsFragment"
        android:name="xyz.zedler.patrick.grocy.fragment.SettingsFragment"
        android:label="SettingsFragment">
        <action
            android:id="@+id/action_settingsFragment_to_logFragment"
            app:destination="@id/logFragment"
            app:enterAnim="@anim/slide_from_right"
            app:exitAnim="@anim/slide_to_left"
            app:popEnterAnim="@anim/slide_from_left"
            app:popExitAnim="@anim/slide_to_right" />
        <argument
            android:name="showCategory"
            app:argType="string" />
        <action
            android:id="@+id/action_settingsFragment_self"
            app:destination="@id/settingsFragment"
            app:enterAnim="@anim/slide_from_right"
            app:exitAnim="@anim/slide_to_left"
            app:popEnterAnim="@anim/slide_from_left"
            app:popExitAnim="@anim/slide_to_right" />
    </fragment>
</navigation>